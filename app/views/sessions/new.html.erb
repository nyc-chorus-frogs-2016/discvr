<%= form_tag session_path do %>

  <legend>Log in</legend>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag 'Log in', class: 'button'%>
  </div>


<% end %>

<%= link_to 'No account? Sign up', new_user_path %>
